{% capture section_content %}
  <ul class="project">
    {% for project in section_data.projects %}
      {% render 'snippets/project-card', project: project, tag: 'li' %}
    {% endfor %}
  </ul>
{% endcapture %}

{% render 'snippets/section',
  section_title: section_data.title,
  section_content: section_content
%}
