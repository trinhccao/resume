<section class="section">
  <div class="container">
    <h2 class="heading-lv2">{{ section_data.title }}</h2>
    <ul class="projects">
      {% for project in section_data.projects %}
        <li class="projects__item">
          <h3 class="label label--projects">{{ project.title }}</h3>
          {% render 'modules/project-card', project: project %}
        </li>
      {% endfor %}
    </ul>
  </div>
</section>